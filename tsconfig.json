{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"declaration": true,
		"emitDeclarationOnly": true,
		"skipLibCheck": true,
		"outDir": "dist",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"jsx": "preserve",
		"jsxImportSource": "solid-js",
		"strict": true,
		"isolatedModules": true,
		"isolatedDeclarations": true
	},
	"include": ["src"]
}
